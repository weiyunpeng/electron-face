@charset "UTF-8";
.debug_show { display: none; }

* { margin: 0; padding: 0; outline: none; }

*:not(input, textarea) { -webkit-touch-callout: inherit; -webkit-user-select: auto; -webkit-text-size-adjust: none; }

ul, dl, dt, dd, h1, h2, h3, h4, h5, h6, p, form { padding: 0; margin: 0; }

img { border: none; display: block; }

html { font-size: 62.5%; font-family: "Open Sans", Arial, "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", "STHeiti", "WenQuanYi Micro Hei", SimSun, sans-serif; -webkit-text-size-adjust: 100%; -moz-text-size-adjust: 100%; -ms-text-size-adjust: 100%; text-size-adjust: 100%; width: 100%; height: 100%; }

body { position: absolute; top: 0; bottom: 0; right: 0; left: 0; font-size: 12px; width: 1920px; height: 100%; overflow: hidden; background: url("../../images/bg.png") center center no-repeat; background-size: cover; }
