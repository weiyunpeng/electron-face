body #main_title .light2 { position: absolute; top: 20px; left: -13%; animation: light2 2s infinite; -webkit-animation: light2 2s infinite; animation-direction: alternate; -webkit-animation-direction: alternate; }
@-webkit-keyframes light2 { 0% { left: 18%; }
  100% { left: -10%; } }
