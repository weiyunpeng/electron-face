body .tips { position: absolute; top: 50%; width: 100%; height: 100%; overflow: hidden; z-index: 999; text-align: center; }
body .tips .tips_fail { animation: tips 100ms ease-in-out 8; }
@-webkit-keyframes tips { 0% { transform: translate(0px, 0px) rotate(0deg); }
  20% { transform: translate(1.5px, -2.5px) rotate(-1.5deg); }
  40% { transform: translate(-2.5px, 0.5px) rotate(-0.5deg); } }
@-moz-keyframes tips { 0% { transform: translate(0px, 0px) rotate(0deg); }
  20% { transform: translate(1.5px, -2.5px) rotate(-1.5deg); }
  40% { transform: translate(-2.5px, 0.5px) rotate(-0.5deg); } }
@-o-keyframes tips { 0% { transform: translate(0px, 0px) rotate(0deg); }
  20% { transform: translate(1.5px, -2.5px) rotate(-1.5deg); }
  40% { transform: translate(-2.5px, 0.5px) rotate(-0.5deg); } }
@keyframes tips { 0% { transform: translate(0px, 0px) rotate(0deg); }
  20% { transform: translate(1.5px, -2.5px) rotate(-1.5deg); }
  40% { transform: translate(-2.5px, 0.5px) rotate(-0.5deg); } }
body .tips .tips_success { animation: tips1 1.2s ease-in-out 1; -webkit-animation: tips1 1.2s ease-in-out 1; }
@-webkit-keyframes tips1 { 0% { opacity: 0.5; }
  20% { opacity: 0; }
  40% { opacity: 1; } }
@-moz-keyframes tips1 { 0% { opacity: 0.5; }
  20% { opacity: 0; }
  40% { opacity: 1; } }
@-o-keyframes tips1 { 0% { opacity: 0.5; }
  20% { opacity: 0; }
  40% { opacity: 1; } }
@keyframes tips1 { 0% { opacity: 0.5; }
  20% { opacity: 0; }
  40% { opacity: 1; } }
