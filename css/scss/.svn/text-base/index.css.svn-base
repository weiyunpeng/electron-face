body #main_title .light1 { position: absolute; top: -64px; left: 13%; animation: light1 2s infinite; -webkit-animation: light1 2s infinite; animation-direction: alternate; -webkit-animation-direction: alternate; }
@-webkit-keyframes light1 { 0% { left: -10%; }
  100% { left: 18%; } }
@-moz-keyframes light1 { 0% { left: -10%; }
